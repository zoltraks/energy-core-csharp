<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Energy.Core</AssemblyName>
    <Version>17.10.2.1</Version>
    <FileVersion>17.10.2.1</FileVersion>
    <AssemblyVersion>2017.10.12</AssemblyVersion>
    <TargetFrameworks>netstandard2.0;net40;net20</TargetFrameworks>
    <ApplicationIcon>Application.ico</ApplicationIcon>
    <RootNamespace>Energy</RootNamespace>
    <Authors>Filip Golewski &lt;f.golewski@gmail.com&gt;</Authors>
    <Company />
    <Product />
    <Description></Description>
    <Copyright>Filip Golewski &lt;f.golewski@gmail.com&gt;</Copyright>
    <PackageReleaseNotes>How much reality can you take?</PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageProjectUrl>http://energy.alyx.pl/</PackageProjectUrl>
    <PackageIconUrl>http://energy.alyx.pl/media/icon/nuget.png</PackageIconUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\</OutputPath>
    <DocumentationFile>bin\Energy.Core.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <DefineConstants>TRACE;RELEASE;NETSTANDARD</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\</OutputPath>
    <DocumentationFile>bin\Energy.Core.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <DefineConstants>TRACE;DEBUG;NETSTANDARD</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.0|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard2.0|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
